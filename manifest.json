{
  "manifest_version": 3,
  "name": "Selected Content Extractor",
  "version": "1.0",
  "description": "Extracts selected content from the active tab.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
